{
  "expo": {
    "entryPoint": "./src/Root.js",
    "name": "notes-debts",
    "slug": "notes-debts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./icons/icon.png",
    "splash": {
      "image": "./icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./icons/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./icons/favicon.png"
    }
  }
}
